package masivi;
import java.util.Scanner;
public class zad1 {

	public static void ArrayInput (int [] arr)
	{	
		Scanner inp = new Scanner (System.in);
		for (int i = 0; i<arr.length; i++) 
		{
			arr[i] = inp.nextInt();
		}
		
	}
	
	public static void ArrayOutput (int[]arr) 
	{
		for (int i = 0 ; i<arr.length;i++) 
		{
			System.out.println(arr[i]);
		}
	}
	
	public static void HigherAverage (int [] arr) 
	{
		int brHigherAvg = 0;
		double sum = 0;
		
		for (int i = 0; i<arr.length;i++) 
		{
			 sum+=arr[i];
		}
		
		double average = sum / (double) arr.length;
		
		for (int i = 0; i<arr.length;i++) 
		{
			if ((double)arr[i] > average) {brHigherAvg+=1;}
		}
		
		System.out.println("Number of elements higher than average: " + brHigherAvg);
	}
	
	public static void main(String[] args) {
		
		Scanner inp = new Scanner (System.in);
		
		int n = inp.nextInt();
		int [] arr  = new int [n];
		
		ArrayInput(arr);
		System.out.println("-");
		ArrayOutput(arr);
		System.out.println("-");
		HigherAverage(arr);
		
	}

}
